<article class="standard-container">
  <p class="breadcrumb">
    <a class="breadcrumb-link" href="/curriculum">curriculum/</a><a class="breadcrumb-link" href="/curriculum/{{data.year.slug}}">{{data.year.slug}}</a>/{{filters.course}}
  </p>

  <div class="course-types">
    {{#each data.courseType}}
    <div class="course-type">
      <a href="{{slug}}"><p>{{title}}</p></a>
    </div>
    {{/each}}
  </div>

  <h2>{{data.year.title}}</h2>
  <div class="year">
    <div class="courses">
      {{# each data.course}}
      <div class="course {{slug}}" style="background-image: url({{cloudinaryUrl image}})">
        <a href="{{category}}/{{slug}}"><h3>{{title}}</h3></a>
      </div>

      {{/each}}
    </div>
  </div>
</article>
